package opg1;

public class FizzBuzz {

	public static void DoFizzBuzz(int[] numbers) {
		for (int n : numbers) {
			boolean divisibleBy3, divisibleBy5;
			divisibleBy3 = IsDivisibleBy(n, 3);
			// If divisible by 3.
			if (divisibleBy3) {
				divisibleBy5 = IsDivisibleBy(n, 5);
				// If divisible by 3 and 5.
				if (divisibleBy5) {
					
				}
				return;
			}
		}
	}
	
	private static boolean IsDivisibleBy(int number, int dividedBy) {
		return number % dividedBy == 0;
	}
	
}
