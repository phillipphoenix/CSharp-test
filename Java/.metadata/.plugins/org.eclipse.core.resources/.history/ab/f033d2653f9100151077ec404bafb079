package opg2;

import java.util.ArrayList;
import java.util.List;

public class Product {

	private String productNb;
	private String name;
	private String description;
	/**
	 * List of prices for this product. Current price is the price that has the most recent fromDate.
	 */
	private List<Price> prices;
	
	public Product(String productNb, String name, String description) {
		super();
		this.productNb = productNb;
		this.name = name;
		this.description = description;
		this.prices = new ArrayList<Price>();
	}
	
	public Price getCurrentPrice() {
		prices
	}

	public String getProductNb() {
		return productNb;
	}

	public void setProductNb(String productNb) {
		this.productNb = productNb;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public List<Price> getPrices() {
		return prices;
	}

	public void setPrices(List<Price> prices) {
		this.prices = prices;
	}
	
}
